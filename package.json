    "eleventy-critical-css": "^0.2.2",
    "gulp": "^4.0.2",
    "gulp-run-command": "^0.0.10",
    "gulp-sass": "^4.1.0",
    "js-yaml": "^4.0.0",
    "katex": "^0.12.0",
    "markdown-it-for-inline": "^0.1.1",
    "markdown-it-implicit-figures": "^0.10.0",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.1",
    "sharp": "^0.30.5"
  }
